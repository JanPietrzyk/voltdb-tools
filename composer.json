    "description": "A Collection of tools intended to make using VoltDB with PHP simpler. Part of a proof of concept to create a Doctrine DBAL driver.",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "~4.5",
        "phpunit/phpunit-skeleton-generator": "*",
        "phpdocumentor/phpdocumentor": "2.*",
        "phpdocumentor/template-responsive-twig": "*"
    },
    "autoload": {
        "psr-4": {
            "Jpietrzyk\\VoltDbTools\\": "src/"
        }
    },
